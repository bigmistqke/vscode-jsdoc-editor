{
  "name": "vscode-jsdoc-editor",
  "author": "bigmistqke",
  "version": "0.1.0",
  "description": "",
  "engines": {
    "vscode": "^1.23.0"
  },
  "activationEvents": [
    "onCommand:jsdoc-editor.start"
  ],
  "main": "./build/extension/index.js",
  "contributes": {
    "commands": [
      {
        "command": "jsdoc-editor.start",
        "title": "Open",
        "category": "Jsdoc Editor"
      },
      {
        "command": "jsdoc-editor-dev.start",
        "title": "Open (Dev)",
        "category": "Jsdoc Editor"
      }
    ]
  },
  "scripts": {
    "start": "vite",
    "dev": "vite",
    "build": "vite build && tsc -p src/extension/tsconfig.json",
    "serve": "vite preview"
  },
  "license": "MIT",
  "devDependencies": {
    "@types/glob": "^7.1.3",
    "@types/jsdom": "^21.1.7",
    "@types/node": "^12.11.7",
    "@types/unist": "^3.0.2",
    "@types/vscode": "^1.46.0",
    "solid-devtools": "^0.29.2",
    "typescript": "^5.3.3",
    "vite": "^5.0.11",
    "vite-plugin-solid": "^2.8.2"
  },
  "dependencies": {
    "@solid-primitives/list": "^0.0.100",
    "@solid-primitives/resource": "^0.1.2",
    "@types/vscode-webview": "^1.57.5",
    "@vscode/codicons": "^0.0.36",
    "clsx": "^2.1.1",
    "colord": "^2.9.3",
    "comment-parser": "^1.4.1",
    "jsdom": "^24.1.0",
    "shiki": "^1.6.2",
    "solid-js": "^1.8.11",
    "solid-presence": "^0.1.6",
    "solid-shiki-textarea": "^0.1.1",
    "solid-textarea-autosize": "^0.0.5",
    "unist-util-visit": "^5.0.0",
    "vite-tsconfig-paths": "^4.3.2"
  }
}
