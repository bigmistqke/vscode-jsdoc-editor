{
  "name": "vscode-webview-solid",
  "author": "bigmistqke",
  "version": "0.1.0",
  "description": "",
  "engines": {
    "vscode": "^1.23.0"
  },
  "activationEvents": [
    "onCommand:solid-webview.start"
  ],
  "main": "./build/extension/index.js",
  "contributes": {
    "commands": [
      {
        "command": "solid-webview.start",
        "title": "Start Solid Webview",
        "category": "Solid"
      },
      {
        "command": "solid-webview-dev.start",
        "title": "Start Solid Webview (Dev)",
        "category": "Solid"
      }
    ]
  },
  "scripts": {
    "start": "vite",
    "dev": "vite",
    "build": "vite build && tsc -p src/extension/tsconfig.json",
    "serve": "vite preview"
  },
  "license": "MIT",
  "devDependencies": {
    "@types/glob": "^7.1.3",
    "@types/node": "^12.11.7",
    "@types/vscode": "^1.46.0",
    "solid-devtools": "^0.29.2",
    "typescript": "^5.3.3",
    "vite": "^5.0.11",
    "vite-plugin-solid": "^2.8.2"
  },
  "dependencies": {
    "@types/vscode-webview": "^1.57.5",
    "solid-js": "^1.8.11"
  }
}
